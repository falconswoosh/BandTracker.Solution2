@{
  Layout = "_Layout";
}

<h1 id="stylists">Stylists</h1>
<ul>

  @if (Model.Count == 0)
  {
  <p> Stylist List is empty</p>
  }
  @foreach (var stylist in Model)
  {
    <li class="stylistsOut"><a href='/stylist/@stylist.GetId()'>@stylist.GetName()</a></li>
  }
</ul>
<a href="/">Back Home</a><br>
